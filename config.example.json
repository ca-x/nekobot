{
  "agents": {
    "defaults": {
      "workspace": "~/.nekobot/workspace",
      "provider": "anthropic",
      "model": "claude-3-5-sonnet-20241022",
      "max_tokens": 4096,
      "temperature": 0.7,
      "max_tool_iterations": 10
    }
  },
  "providers": {
    "anthropic": {
      "api_key": "",
      "api_base": "https://api.anthropic.com",
      "rotation": {
        "enabled": true,
        "strategy": "round_robin",
        "cooldown": "5m"
      },
      "profiles": {
        "primary": {
          "api_key": "sk-ant-xxx-primary",
          "priority": 1
        },
        "secondary": {
          "api_key": "sk-ant-xxx-secondary",
          "priority": 2
        },
        "backup": {
          "api_key": "sk-ant-xxx-backup",
          "priority": 3
        }
      }
    },
    "openai": {
      "api_key": "",
      "api_base": "https://api.openai.com/v1",
      "rotation": {
        "enabled": true,
        "strategy": "least_used",
        "cooldown": "10m"
      },
      "profiles": {
        "prod": {
          "api_key": "sk-xxx-prod",
          "priority": 1
        },
        "dev": {
          "api_key": "sk-xxx-dev",
          "priority": 2
        }
      }
    },
    "google": {
      "api_key": "",
      "api_base": "https://generativelanguage.googleapis.com"
    },
    "openrouter": {
      "api_key": "",
      "api_base": "https://openrouter.ai/api/v1",
      "comment": "OpenRouter - Access to multiple models through one API"
    },
    "deepseek": {
      "api_key": "",
      "api_base": "https://api.deepseek.com/v1",
      "comment": "DeepSeek API - OpenAI compatible"
    },
    "together": {
      "api_key": "",
      "api_base": "https://api.together.xyz/v1",
      "comment": "Together AI - OpenAI compatible API"
    },
    "groq": {
      "api_key": "",
      "api_base": "https://api.groq.com/openai/v1",
      "comment": "Groq - Ultra-fast LLM inference"
    },
    "ollama": {
      "api_key": "ollama",
      "api_base": "http://localhost:11434/v1",
      "comment": "Ollama local models - OpenAI compatible API"
    },
    "lmstudio": {
      "api_key": "lm-studio",
      "api_base": "http://localhost:1234/v1",
      "comment": "LM Studio local server - OpenAI compatible"
    },
    "vllm": {
      "api_key": "vllm",
      "api_base": "http://localhost:8000/v1",
      "comment": "vLLM self-hosted - OpenAI compatible API"
    },
    "custom": {
      "api_key": "your-api-key",
      "api_base": "https://your-custom-api.com/v1",
      "comment": "Any OpenAI-compatible API endpoint"
    }
  },
  "gateway": {
    "host": "0.0.0.0",
    "port": 8080
  },
  "channels": {
    "telegram": {
      "enabled": false,
      "bot_token": "",
      "allow_from": [],
      "proxy": ""
    },
    "discord": {
      "enabled": false,
      "bot_token": "",
      "allow_from": [],
      "proxy": ""
    },
    "slack": {
      "enabled": false,
      "bot_token": "",
      "app_token": "",
      "allow_from": []
    },
    "whatsapp": {
      "enabled": false,
      "bridge_url": "ws://localhost:3000",
      "allow_from": []
    },
    "feishu": {
      "enabled": false,
      "app_id": "",
      "app_secret": "",
      "encrypt_key": "",
      "verification_token": "",
      "allow_from": []
    },
    "dingtalk": {
      "enabled": false,
      "client_id": "",
      "client_secret": "",
      "allow_from": []
    },
    "qq": {
      "enabled": false,
      "app_id": "",
      "app_secret": "",
      "allow_from": []
    },
    "maixcam": {
      "enabled": false,
      "host": "0.0.0.0",
      "port": 8765,
      "allow_from": []
    },
    "serverchan": {
      "enabled": false,
      "bot_token": "",
      "allow_from": []
    },
    "wework": {
      "enabled": false,
      "corp_id": "",
      "agent_id": "",
      "corp_secret": "",
      "token": "",
      "encoding_aes_key": "",
      "allow_from": []
    },
    "googlechat": {
      "enabled": false,
      "project_id": "",
      "credentials_file": "",
      "allow_from": []
    }
  },
  "tools": {
    "exec": {
      "enabled": true,
      "allow_commands": ["ls", "cat", "grep", "find", "git"],
      "timeout": 30
    },
    "web_search": {
      "enabled": true,
      "brave_api_key": ""
    },
    "web_fetch": {
      "enabled": true,
      "timeout": 30
    },
    "file_operations": {
      "enabled": true,
      "max_file_size": 10485760
    }
  },
  "heartbeat": {
    "enabled": false,
    "interval_minutes": 60
  },
  "memory": {
    "qmd": {
      "enabled": false,
      "command": "qmd",
      "include_default": true,
      "paths": [
        {
          "name": "docs",
          "path": "~/Documents",
          "pattern": "**/*.md"
        }
      ],
      "sessions": {
        "enabled": true,
        "export_dir": "${WORKSPACE}/memory/sessions",
        "retention_days": 90
      },
      "update": {
        "on_boot": true,
        "interval": "30m",
        "command_timeout": "30s",
        "update_timeout": "5m"
      }
    }
  },
  "skills": {
    "auto_reload": true,
    "builtin_enabled": true
  }
}

    "timeout_seconds": 60
  },
  "tools": {
    "exec": {
      "timeout_seconds": 30,
      "sandbox": {
        "enabled": false,
        "image": "alpine:3.20",
        "network_mode": "none",
        "mounts": [],
        "timeout": 60,
        "auto_cleanup": true
      }
    },
    "web": {
      "search": {
        "api_key": "",
        "max_results": 5
      },
      "fetch": {
        "max_chars": 50000
      }
    }
  },
  "heartbeat": {
    "enabled": false,
    "interval_minutes": 60
  },
  "redis": {
    "addr": "localhost:6379",
    "password": "",
    "db": 0
  },
  "state": {
    "backend": "file",
    "file_path": "",
    "prefix": "nekobot:"
  },
  "bus": {
    "type": "local",
    "prefix": "nekobot:bus:"
  },
  "memory": {
    "qmd": {
      "enabled": false,
      "command": "qmd",
      "include_default": true,
      "paths": [
        {
          "name": "docs",
          "path": "~/Documents",
          "pattern": "**/*.md"
        }
      ],
      "sessions": {
        "enabled": true,
        "export_dir": "${WORKSPACE}/memory/sessions",
        "retention_days": 90
      },
      "update": {
        "on_boot": true,
        "interval": "30m",
        "command_timeout": "30s",
        "update_timeout": "5m"
      }
    }
  },
  "webui": {
    "enabled": true,
    "port": 0,
    "public_base_url": "",
    "secret": "",
    "username": "admin",
    "password": "",
    "tool_session_otp_ttl_seconds": 180
  }
}
